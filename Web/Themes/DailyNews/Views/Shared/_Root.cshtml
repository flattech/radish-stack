@using Web.Models.UI
@{ var themepath = "/Themes/DailyNews/Content";}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <!--title-->
    @*<title>@Html.RenderTitle(false)</title>*@
    <title>Daily News</title>

    <!--CSS-->
    <link href="@themepath/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="@themepath/css/magnific-popup.css" rel="stylesheet">
    <link href="@themepath/css/owl.carousel.css" rel="stylesheet">
    <link href="@themepath/css/main.css" rel="stylesheet">
    <link href="@themepath/css/responsive.css" rel="stylesheet">

    <!--Google Fonts-->
    <link href='https://fonts.googleapis.com/css?family=Signika+Negative:400,300,600,700' rel='stylesheet' type='text/css'>
    <link rel="shortcut icon" href="@themepath/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@themepath/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@themepath/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@themepath/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="@themepath/images/ico/apple-touch-icon-57-precomposed.png">
</head><!--/head-->

<body>

    <div id="main-wrapper">
        @Html.Action("Header", "Shared")
        <div class="container-fluid">
            @RenderBody()
        </div>
        @Html.Action("Footer", "Shared")
    </div>
    <script type="text/javascript" src="@themepath/js/jquery.js"></script>
    <script type="text/javascript" src="@themepath/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="@themepath/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="@themepath/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="@themepath/js/main.js"></script>

</body>
</html>
